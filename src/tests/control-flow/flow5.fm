fn test_void_early_return(x: i32) -> void {
    if x < 0 {
        return;
    }
    
    let y = x * 2;
}

// Edge cases
fn test_multiple_returns() -> i32 {
    return 1;
    return 2; // Unreachable
    return 3; // Unreachable
}

fn test_deep_nesting(a: i32, b: i32, c: i32) -> i32 {
    if a > 0 {
        if b > 0 {
            if c > 0 {
                return 1;
            }
            else {
                return 2;
            }
        }
        else {
            return 3;
        }
    }
    else {
        return 4;
    }
}

